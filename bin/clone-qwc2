#!/bin/sh
startdir=`pwd`
cd /usr/local/src
if [ $EUID -eq 0 ]
then
    chpst -u ${USER} -b argv0 -b argv1 rm -rf qwc2-demo-app
    chpst -u ${USER} -b argv0 -b argv1 -b argv2 -b argv3 -b argv4 git clone --recursive -b ${QWC2_GIT_BRANCH} ${QWC2_GIT_REPOSITORY}
else
    rm -rf qwc2-demo-app
    git clone --recursive -b ${QWC2_GIT_BRANCH} ${QWC2_GIT_REPOSITORY}
fi
cd $startdir
